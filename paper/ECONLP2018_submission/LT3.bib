@INCOLLECTION{vanOosten2011,
  author = {van Oosten, Philip and Hoste, V\'eronique and Tanghe, Dries},
  title = {A Posteriori Agreement as a Quality Measure for Readability Prediction
	Systems},
  booktitle = {Computational Linguistics and Intelligent Text Processing},
  publisher = {Springer Berlin / Heidelberg},
  year = {2011},
  editor = {Gelbukh, Alexander},
  volume = {6609},
  series = {Lecture Notes in Computer Science},
  pages = {424-435},
  note = {10.1007/978-3-642-19437-5_35},
  abstract = {All readability research is ultimately concerned with the research
	question whether it is possible for a prediction system to automatically
	determine the level of readability of an unseen text. A significant
	problem for such a system is that readability might depend in part
	on the reader. If different readers assess the readability of texts
	in fundamentally different ways, there is insufficient a priori agreement
	to justify the correctness of a readability prediction system based
	on the texts assessed by those readers. We built a data set of readability
	assessments by expert readers. We clustered the experts into groups
	with greater a priori agreement and then measured for each group
	whether classifiers trained only on data from this group exhibited
	a classification bias. As this was found to be the case, the classification
	mechanism cannot be unproblematically generalized to a different
	user group.},
  affiliation = {LT3 Language and Translation Technology Team, University College Ghent,
	Groot-Brittannilaan 45, 9000 Ghent, Belgium},
  isbn = {978-3-642-19436-8},
  owner = {philip},
  timestamp = {2011.02.18},
  url = {http://dx.doi.org/10.1007/978-3-642-19437-5_35}
}

